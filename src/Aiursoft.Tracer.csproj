<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>Aiursoft.Tracer</AssemblyName>
    <RootNamespace>Aiursoft.Tracer</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <IsPackable>false</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Aiursoft Tracer is a simple network quality testing app. Helps testing the connection speed between you and the server.</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Aiursoft.AiurObserver.WebSocket.Server" Version="8.0.8" />
    <PackageReference Include="Aiursoft.Scanner" Version="8.0.2" />
    <PackageReference Include="Aiursoft.WebTools" Version="8.0.28" />
  </ItemGroup>
</Project>